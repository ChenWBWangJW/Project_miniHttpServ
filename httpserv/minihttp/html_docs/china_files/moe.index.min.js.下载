function setTab(t,e,n){for(i=1;i<=n;i++){var o=document.getElementById(t+i),a=document.getElementById(t+"_con"+i);o.className=i==e?"xxyw":"",a.style.display=i==e?"block":"none"}}function focusRotate(t,e){cover.hide();var i=t<0?itemLength+t:t,n=i-1<0?itemLength-1:i-1,o=i+1>=itemLength?0:i+1;e?rotateItem.find("p").hide().end().css({background:"none",left:"190px",top:"32px",padding:0,width:"610px",height:"370px",opacity:0,"z-index":0}).eq(i).css({left:"-145px","z-index":3,opacity:1}).stop().animate({top:0,left:0,padding:"0 145px",width:"700px",height:"435px","background-position":0},{duration:durTime,easing:"linear"},useTime).find("img").hover(function(){$(this).css({transform:"scale(1.1)",transition:"all 0.5s"})},function(){$(this).css({transform:"scale(1)",transition:"all 0.5s"})}).end().find("p").show().end().end().eq(n).css({"z-index":1,opacity:.5}).stop().animate({left:0},{duration:durTime,easing:"linear"},useTime).end().eq(o).css({left:0,top:0,width:"700px",height:"435px","padding-left":"145px","z-index":1,opacity:.5}).stop().animate({top:"32px",left:"380px",width:"610px",height:"370px","padding-left":0},{duration:durTime,easing:"linear"},useTime):rotateItem.find("p").hide().end().css({background:"none",left:"190px",top:"32px",padding:0,width:"610px",height:"370px",opacity:0,"z-index":0}).eq(n).css({top:0,left:0,width:"700px",height:"435px","z-index":1,opacity:.5,padding:"0 145px"}).stop().animate({top:"32px",padding:0,width:"610px",height:"370px"},{duration:durTime,easing:"linear"},useTime).end().eq(i).css({left:0,"padding-left":"380px","z-index":3,opacity:1}).stop().animate({top:0,padding:"0 145px",width:"700px",height:"435px"},{duration:durTime,easing:"linear"},useTime).find("img").hover(function(){$(this).css({transform:"scale(1.1)",transition:"all 0.5s"})},function(){$(this).css({transform:"scale(1)",transition:"all 0.5s"})}).end().find("p").show().end().end().eq(o).css({"z-index":1,opacity:.5}).stop().animate({left:"380px"},{duration:durTime,easing:"linear"},useTime),pagenation.removeClass("curr").eq(i).addClass("curr"),cover.fadeIn(1500)}var focusObj=$("#jyb_index_focus"),focusBox=$(".jyb_index_focusImg"),rotateItem=$(".rotate-item"),itemLength=rotateItem.size(),pagenation=$(".focus-pagenation").find("dd"),rotateBtnPr=$(".focus-arrow-left"),rotateBtnNx=$(".focus-arrow-right"),useTime=5e3,durTime=500,cover=$("#moe-focus-cover");rotateItem.last().css("opacity","0.5"),$(".active").find("img").hover(function(){$(this).css({transform:"scale(1.1)",transition:"all 0.5s"})},function(){$(this).css({transform:"scale(1)",transition:"all 0.5s"})});var activeIndex=0,focusTime=setInterval("focusRotate(++activeIndex%itemLength)",useTime);focusObj.mouseenter(function(){focusTime&&clearInterval(focusTime),rotateBtnPr.stop().animate({left:0,opacity:1},500),rotateBtnNx.stop().animate({right:0,opacity:1},500)}).mouseleave(function(){focusTime&&clearInterval(focusTime),focusTime=setInterval("focusRotate(++activeIndex%itemLength)",useTime),rotateBtnPr.stop().animate({left:"-60px",opacity:0},500),rotateBtnNx.stop().animate({right:"-60px",opacity:0},500)}),pagenation.click(function(){focusTime&&clearInterval(focusTime);var t=$(this).index();focusRotate(t%itemLength,!(t>activeIndex)),activeIndex=t}),rotateBtnPr.stop().click(function(){focusTime&&clearInterval(focusTime),focusRotate(--activeIndex%itemLength,!0)}),rotateBtnNx.stop().click(function(){focusTime&&clearInterval(focusTime),focusRotate(++activeIndex%itemLength)}),$(function(){var t=$(".scy-nav").find("ul").find("li");t.hover(function(){t.find("dl").stop(!1,!0).hide(),$(this).find("dl").slideDown(500)},function(){$(this).find("dl").stop().slideUp(200)});var e=$("#tt3").find("a");e.eq(1).css("color","#6b6b6b").end().hover(function(){e.css("color","#6b6b6b"),$(this).css("color","#567fe6")},function(){e.css("color","#6b6b6b")});var i=new Swiper(".swiper_hot_news",{loop:!0,calculateHeight:!0,speed:1e3,autoplay:5e3,autoplayDisableOnInteraction:!1,pagination:".hot-pagination",paginationClickable:!0,onInit:function(t){},onTouchStart:function(t){},onTouchEnd:function(t){},onResistanceAfter:function(t,e){}});$(window).resize(function(){$(window).width()<=992&&i.reInit()});var n=$(".hot-btn-prev"),o=$(".hot-btn-next");n.click(function(){i.swipePrev()}),o.click(function(){i.swipeNext()});$("#navigation").find("li").mouseenter(function(){$(this).find("ul").show()}).mouseleave(function(){$(this).find("ul").hide()}),$.ajax({url:"http://www.gov.cn/pushinfo/v150203/pushinfo.jsonp",dataType:"jsonp",jsonp:"pushInfoJsonpCallBack",jsonpCallback:"pushInfoJsonpCallBack",success:function(t){$.each(t,function(t,e){if(t>7)return!1;var i=e.title;i.length>44&&(i=i.substring(0,44)+"...");var n=e.pubDate.split("-"),o=n[1]+"-"+n[2];$("#testUl").append("<li><span>"+o+"</span><a href='"+e.link+"' target='_blank'>"+i+"</a></li>")})},error:function(){}});var a=$(".index_moe_gwyxw").find("a"),s=$("#eight_con1").find("ul");a.mouseover(function(){a.css("color","#6b6b6b"),$(this).css("color","#567fe6"),"eight1"==$(this).parent().attr("id")&&(s.hide(),s.eq($(this).index()).show())})});for(var yg=$(".jyb_yg"),i=0;i<yg.size();i++)"#"==yg.eq(i).attr("href")&&yg.eq(i).removeAttr("href").removeClass("outMoeLink");for(var gwy=$("#moe_index_foot_gwy").find("a"),i=0;i<gwy.size();i++)"#"==gwy.eq(i).attr("href")&&gwy.eq(i).removeAttr("href");var mySwiperTopFocus=new Swiper(".swiper_container_top_Focus",{loop:!0,calculateHeight:!0,speed:1e3,autoplay:5e3,autoplayDisableOnInteraction:!1,pagination:".pagination",paginationClickable:!0,onInit:function(t){},onTouchStart:function(t){},onTouchEnd:function(t){},onResistanceAfter:function(t,e){}});$(window).resize(function(){$(window).width()<=992&&mySwiperTopFocus.reInit()});